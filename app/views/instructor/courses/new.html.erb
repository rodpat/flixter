<br>
<div class="booyah-box col-10 ml-auto mr-auto">
  <h1>Add a new course</h1>
  <br>
  <%= simple_form_for @course, url: instructor_courses_sections_path(current_section) do |f| %>
    <%= f.input :title %>
    <%= f.input :description %>
    <%= f.input :cost %>
    <br>
    <%= f.submit "Create", class: 'btn btn-primary' %>
    <br>
  <% end %>
</div>